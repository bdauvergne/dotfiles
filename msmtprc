# Set default values for all following accounts.
defaults
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/.local/var/msmtp.log

# Passwords are stored in gnome-keyring,

# A mail service
account Entrouvert
host mail.entrouvert.org
from bdauvergne@entrouvert.com
tls_starttls on
port 587
auth on
user bdauvergne
# Set a default account
account default : Entrouvert

